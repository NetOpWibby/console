{
  "author": {
    "name": "netop://ウエハ",
    "url": "https://webb.page"
  },
  "description": "Print console messages in STYLE.",
  "devDependencies": {
    "@webb/config-nyc": "^2020.3.29",
    "@webb/config-prettier": "^2020.3.29",
    "@webb/test": "^2020.3.29",
    "chronver": "^2020.3.9",
    "del-cli": "^3.0.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "updates": "^10.2.7"
  },
  "homepage": "https://github.com/NetOperatorWibby/console",
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run pretty && npm run increment && git add -A :/"
    }
  },
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@webb/console",
  "prettier": "@webb/config-prettier",
  "repository": {
    "type": "git",
    "url": "https://github.com/NetOperatorWibby/console"
  },
  "scripts": {
    "build": "npm run cleanup && tsc",
    "cleanup": "del './dist'",
    "increment": "chronver --increment package",
    "pretty": "prettier --write 'dist/**/*.js' 'dist/**/*.ts'",
    "showcase": "npm run build && ts-node showcase.ts",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./",
    "test:typescript": "tsc",
    "test:assert": "ts-node test/index.ts",
    "test:coverage": "nyc npm run test:assert"
  },
  "types": "dist/index.d.ts",
  "version": "2020.04.15"
}
